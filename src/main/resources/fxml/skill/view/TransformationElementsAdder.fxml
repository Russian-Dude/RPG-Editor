   <children>
      <HBox prefHeight="25.0" prefWidth="142.0">
         <children>
            <ComboBox fx:id="elements" onAction="#setSelectedElement" prefWidth="150.0" promptText="NEUTRAL" />
            <Button mnemonicParsing="false" onAction="#remove" text="X" />
         </children>
      </HBox>
   </children>
</Pane>
